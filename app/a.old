
global:
  scrape_interval: 15s
scrape_configs:
    
  - job_name: 'blackbox-icmp-undefined'
    metrics_path: /probe
    params:
      module: [icmp]
    static_configs:
      - targets:

        - 8.8.8.8
        - 1.1.1.1
    relabel_configs:
      - source_labels: [__address__]
        target_label: __param_target
      - source_labels: [__param_target]
        target_label: icmp_lable
      - target_label: __address__
        replacement: undefined:undefined

  - job_name: 'blackbox-dns-undefined'
